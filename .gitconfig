# This is Git's per-user configuration file.
[core]
	excludesfile = ~/.gitignore_global
	editor = vim
	filemode = false
[user]
	name = paikwiki
	email = paikwiki@gmail.com
[init]
	defaultBranch = main
[color]
	ui = true
[pull]
	rebase = false
[credential]
	helper = /usr/local/share/gcm-core/git-credential-manager
[credential "https://dev.azure.com"]
	useHttpPath = true
